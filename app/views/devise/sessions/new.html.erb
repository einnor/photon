    <div id="hm_page">
        <ul class="cb-slideshow">
            <li><span>Image 01</span><div><h3>SMS Notifications.</h3></div></li>
            <li><span>Image 02</span><div><h3>Managing Loans.</h3></div></li>
            <li><span>Image 03</span><div><h3>Member Registration.</h3></div></li>
            <li><span>Image 04</span><div><h3>Loan Disbursement.</h3></div></li>
            <li><span>Image 05</span><div><h3>Handling Contributions.</h3></div></li>
            <li><span>Image 06</span><div><h3>Record Keeping.</h3></div></li>
        </ul>
        <div class="container">
            <!-- Codrops top bar -->
            

            <!--/ Codrops top bar -->
            

		    <div class="form-box" id="login-box">
		        <div class="header">Log In</div>
		    	<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
		    	<div class="body bg-gray">
			    <%= flash_messages %>
                
			    <%- if devise_mapping.omniauthable? %>

				<%= link_to image_tag("twitter_32.png"), omniauth_authorize_path(resource_name, "twitter") %><br />
			    
			    <% end -%>
				
			    <%- if devise_mapping.omniauthable? %>

				<%= link_to image_tag("facebook_32.png"), omniauth_authorize_path(resource_name, "facebook") %><br />
			    
			    <% end -%>    
			
			
			    <%- if devise_mapping.omniauthable? %>

				<%= link_to image_tag("linkedin_32.png"), omniauth_authorize_path(resource_name, "linkedin") %><br />
			    
			    <% end -%>
			    
			    
			    <%- if devise_mapping.omniauthable? %>

				<%= link_to image_tag("google_32.png"), omniauth_authorize_path(resource_name, "google_oauth2") %><br />
			    
			    <% end -%>			    
			
			</div>
			<% end %>

		    </div> 
           
        </div>
    </div>
